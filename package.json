{
  "name": "collaborative-kanban",
  "version": "1.0.0",
  "description": "Real-time collaborative Kanban board application",
  "main": "src/backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:frontend": "webpack serve --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "start": "node src/backend/server-supabase-only.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed-db.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "kanban",
    "collaboration",
    "real-time",
    "websockets",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT",
